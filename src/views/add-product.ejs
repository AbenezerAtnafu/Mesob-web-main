            }).on("files.bs.filedialog", function (event) {
                var html = "";
                for (var a = 0; a < event.files.length; a++) {
                    selectedImages.push(event.files[a]);
                    html += "<img class='img-responsive image-resize' src='" + event.files[a].content + "'>";
