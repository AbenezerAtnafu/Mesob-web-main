<!-- script for filter model and made -->
	<script>

$(document).ready(function(){
            load_make_json_data('makeSelect');
            load_bldg_json_data('bldgCatagorySelect');

            // make function

            function load_make_json_data(id, parent_id){
                let html_code = "";
                $.getJSON('make-data.json', function(data){

                    if(id=='makeSelect'){
                        html_code += "<option value='' selected disabled>Make</option>";
                    }else if(id=='modelSelect'){
                        html_code += "<option value='' selected disabled>Model</option>";
                    }

                    $.each(data, function(key, value){
                        if(id == 'makeSelect'){
                            
                            if(value.parent_id == '0'){
                                html_code += '<option value="'+value.name+'">'+value.name+'</option>';
                            }
                        }else{
                            if(value.parent_id == parent_id){
                                html_code += '<option value="'+value.name+'">'+value.name+'</option>';
                            }
                        }
                        

                        if($("#modelSelect").val() == "Model"){
                            html_code = "sdgbkbh";
                        }
                    });
                    $('#'+id).html(html_code);
                });
            }
            $(document).on('change', '#makeSelect', function(){

                var make_name = $(this).val();
                if(make_name != ''){
                    load_make_json_data('modelSelect', make_name);
                }
                else{
                    $('#makeSelect').html('<option value="">Select Make</option>');
                    $('#modelSelect').html('<option value="">Select Model</option>');
                }

            });


            // bdlg function
            function load_bldg_json_data(id, parent_id){
                let html_code = "";
                $.getJSON('bldg-data.json', function(data){

                    if(id=='bldgCatagorySelect'){
                        html_code += "<option value='' selected disabled>Building Material Catagorey</option>";
                    }else if(id=='bldgDetailSelect'){
                        html_code += "<option value='' selected disabled>Building Material Catagorey</option>";
                    }

                    $.each(data, function(key, value){
                        if(id == 'bldgCatagorySelect'){
                            if(value.parent_id == '0'){
                                html_code += '<option value="'+value.name+'">'+value.name+'</option>';
                                
                            }
                        }else{
                            if(value.parent_id == parent_id){
                                html_code += '<option value="'+value.name+'">'+value.name+'</option>';
                                console.log(html_code);
                            }
                        }
                    });
                    $('#'+id).html(html_code);
                });
            }
            $(document).on('change', '#bldgCatagorySelect', function(){

                var make_name = $(this).val();
                
                if(make_name != ''){
                    load_bldg_json_data('bldgDetailSelect', make_name);
                }
                else{
                    $('#bldgCatagorySelect').html('<option value="">Select Building Supplies Catagory</option>');
                    $('#bldgDetailSelect').html('<option value="">Select Model</option>');
                }

            });

        });


	</script>
